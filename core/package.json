{
  "name": "lunar-core",
  "version": "1.2.3",
  "description": "Authentication, authorization, and user management for Lunar Apps services",
  "main": "core.js",
  "author": "Sean McMenamin <smcmenamin@belunar.com>",
  "license": "ISC",
  "scripts": {
    "start": "yarn staging",
    "staging": "sls offline --stage=staging",
    "qa": "sls offline --stage=qa",
    "demo": "sls offline --stage=demo",
    "prod": "sls offline --stage=production",
    "preinstall": "[ -z \"${SLSPATH}\" ] || ln -f -s ${SLSPATH} ./lib/slsdeploy",
    "deploy": "echo \"CORE DEPLOY USES SLS VERSION $(./lib/slsdeploy --version)\" && yarn sls:package && yarn sls:deploy",
    "sls:package": "./lib/slsdeploy package --stage=${NODE_ENV:=staging} --package build/package",
    "sls:deploy": "./lib/slsdeploy deploy --stage=${NODE_ENV:=staging} --package build/package --verbose --aws-s3-accelerate",
    "sls:deploy:login": "./lib/slsdeploy deploy --stage=${NODE_ENV:=staging} --function loginUser"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^2.0.9",
    "bugsnag": "^2.4.1",
    "ip-range-check": "^0.0.2",
    "joi": "^13.4.0",
    "jsonwebtoken": "^8.2.2",
    "jwk-to-pem": "^2.0.0",
    "knex": "^0.15.2",
    "mysql": "^2.16.0",
    "node-fetch": "^2.1.2",
    "utf8": "^3.0.0",
    "uuid": "^3.2.1",
    "xml-js": "^1.6.9"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.48",
    "@babel/preset-env": "^7.0.0-beta.48",
    "@types/joi": "^14.3.1",
    "@types/node": "^11.12.1",
    "aws-sdk": "^2.504.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0-beta.3",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-security": "^1.4.0",
    "serverless": "1.32.0",
    "serverless-domain-manager": "^2.4.0",
    "serverless-offline": "^3.25.6",
    "serverless-plugin-slack": "awkward/serverless-plugin-slack",
    "serverless-webpack": "^5.1.5",
    "ts-loader": "^5.3.0",
    "typescript": "^3.5.3",
    "webpack": "^4.11.1",
    "webpack-node-externals": "^1.7.2"
  }
}
